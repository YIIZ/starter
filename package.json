{
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode=development --hot --history-api-fallback",
    "build": "webpack --mode=production --progress --hide-modules",
    "cleanup": "rimraf dist/*",
    "prebuild": "yarn cleanup",
    "builddemo": "env $(cat .env/development | grep -v ^# | xargs) yarn build",
    "buildrelease": "env $(cat .env/production | grep -v ^# | xargs) yarn build",
    "deploy": "yarn build && coscp --cache 0 dist demo:demo",
    "package": "yarn buildrelease && pack-it -t ieg -n a20181010xxxx -s ./dist/"
  },
  "devDependencies": {
    "@babel/core": "latest",
